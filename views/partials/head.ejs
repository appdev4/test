       body { padding-top: 65px; margin: 0 auto; }
    
      .col-sm-8 {height:20%;}
      
      #chatbox .panel-default {width:100%}
      
      #chatbox .panel-body{overflow-y: scroll; height: 250px; padding: 2px;}
      
      #chatbox .panel-footer { padding: 6px 5px;}
      #chatbox .panel-footer form { background: #c2c2c2; padding: 3px; bottom: 0; }
      #chatbox .panel-footer form input { border: 0; padding: 10px; width: 80%; margin-right: .5%; }
      #chatbox .panel-footer form button { width: 15%; background: rgb(130, 224, 255); border: none; margin-left:2%; }

      #messages { list-style-type: none; margin: 0; padding: 0; }
      #messages li { padding: 5px 3px; }
      #messages li:nth-child(odd) { background: #eee; }
      
      .col-sm-4 {padding-left:0px;}   
      
